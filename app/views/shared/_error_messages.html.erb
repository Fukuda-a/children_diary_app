<% if object.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
       入力内容に誤りが <%= pluralize(object.errors.count, "つ　あります") %>.
    </div>
    <ul>
    <% @child.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>