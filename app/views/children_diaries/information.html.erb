    <h1 class="page-name">共有事項</h1>
  <tr>
    <td>
      <%= form_with model: @children_diary, :local => true do |f| %>
        <div class="field">
          <%= f.collection_select(:children_id, @children, :id, :name)%>
          <%= f.date_field :date %>
          <div class="text-area">
            <%= f.text_area :information %>
          </div>
        </div>
        <%= f.submit "保存" %>
      <% end %>
    </td>
  </tr>